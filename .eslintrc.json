{
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "extends": "plugin:@tinymce/standard",
      "parserOptions": {
        "project": "tsconfig.json",
        "sourceType": "module"
      },
      "rules": {
        "@tinymce/prefer-fun": "off",
        "semi": "off",
        "@typescript-eslint/semi": "off"
      }
    }
  ]
}
